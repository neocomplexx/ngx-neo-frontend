{
  "name": "@neocomplexx/ngx-neo-frontend-mat",
  "version": "8.1.7",
  "scripts": {
    "build": "cd ../../node_modules/.bin/ && tsc -p ../../projects/ngx-neo-frontend-mat/tsconfig.schematics.json && cd ../../projects/ngx-neo-frontend-mat",
    "copy:schemas": "cp --parents schematics/*/schema.json ../../dist/ngx-neo-frontend-mat/",
    "copy:files": "cp --parents -p -r schematics/*/files/** ../../dist/ngx-neo-frontend-mat/",
    "copy:assets": "cp --parents -p -r schematics/*/assets/** ../../dist/ngx-neo-frontend-mat/",
    "copy:cordova": "cp --parents -p -r schematics/*/cordova/** ../../dist/ngx-neo-frontend-mat/",
    "copy:styles": "cp --parents -p -r schematics/*/styles/** ../../dist/ngx-neo-frontend-mat/",
    "copy:collection": "cp schematics/collection.json ../../dist/ngx-neo-frontend-mat/schematics/collection.json",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:assets && npm run copy:cordova && npm run copy:styles && npm run copy:collection"
  },
  "peerDependencies": {
    "@angular/common": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@neocomplexx/ngx-neo-components-mat": "^8.0.1",
    "@neocomplexx/ngx-neo-completer-mat": "^7.0.1",
    "@neocomplexx/ngx-neo-loader": "^7.0.0",
    "@neocomplexx/ngx-neo-directives-mat": "^7.0.4",
    "@neocomplexx/ngx-neo-modal-mat": "^8.0.0",
    "@neocomplexx/ngx-neo-pipes": "^7.0.5",
    "ngx-treeview": "^6.0.2",
    "@aspnet/signalr": "^1.1.2"
  },
  "schematics": "./schematics/collection.json"
}
