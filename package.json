{
  "name": "neo-frontend",
  "version": "7.0.31",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build ngx-neo-frontend",
    "npm_pack": "cd dist/ngx-neo-frontend && npm pack",
    "package": "npm run build_lib && npm run copy-files &&  npm run build_schematic && npm run npm_pack",
    "debug": "npm run build_lib && ng serve --vendorSourceMap",
    "debug_mat": "npm run build_lib_mat && ng serve --vendorSourceMap",
    "copy-license": "cp ./LICENSE ./dist/ngx-neo-frontend",
    "copy-readme": "cp ./README.md ./dist/ngx-neo-frontend",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "build_schematic": "cd ./projects/ngx-neo-frontend && npm run build",
    "package_mat": "npm run build_lib_mat && npm run copy-files_mat && npm run build_schematic_mat && npm run npm_pack_mat",
    "build_lib_mat": "ng build ngx-neo-frontend-mat",
    "npm_pack_mat": "cd dist/ngx-neo-frontend-mat && npm pack",
    "copy-license_mat": "cp ./LICENSE ./dist/ngx-neo-frontend-mat",
    "copy-readme_mat": "cp ./README.md ./dist/ngx-neo-frontend-mat",
    "copy-files_mat": "npm run copy-license_mat && npm run copy-readme_mat",
    "build_schematic_mat": "cd ./projects/ngx-neo-frontend-mat && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/material": "^7.2.1",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@aspnet/signalr": "^1.1.4",
    "@neocomplexx/ngx-neo-completer": "^7.0.0",
    "@neocomplexx/ngx-neo-completer-mat": "^7.0.1",
    "@neocomplexx/ngx-neo-components": "^7.0.24",
    "@neocomplexx/ngx-neo-components-mat": "^7.0.1",
    "@neocomplexx/ngx-neo-directives": "^7.0.9",
    "@neocomplexx/ngx-neo-directives-mat": "^7.0.0",
    "@neocomplexx/ngx-neo-loader": "^7.0.0",
    "@neocomplexx/ngx-neo-modal": "^7.0.1",
    "@ng-bootstrap/ng-bootstrap": "^4.1.1",
    "@neocomplexx/ngx-neo-modal-mat": "^7.0.1",
    "core-js": "^2.5.4",
    "ng5-breadcrumb": "0.0.6",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/core": "^7.3.6",
    "@angular-devkit/schematics": "^7.3.6",
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "schematics-utilities": "^1.1.2",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
